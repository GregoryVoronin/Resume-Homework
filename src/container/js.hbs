<div>
    <section>
        <h1>{{name}}</h1>
        <p>{{description}}</p>
        <em>Founder: {{history.founder}} ({{history.year}})</em>
        <h2>Types:</h2>
        <ul>
            {{#each types}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <hr>
        <h2>Syntax:</h2>
        {{#with syntax}}
        <h3>Variables</h3>
        <ul>
            {{#each variables}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h3>Functions</h3>
        <ul>
            {{#each functions}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h3>Conditionals</h3>
        <ul>
            {{#each conditionals}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h3>Loops</h3>
        <ul>
            {{#each loops}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h3>Classes</h3>
        <ul>
            {{#each classes}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        {{/with}}
        <hr>
        <h2>Features:</h2>
        <ul>
            {{#each features}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <hr>
        <h2>Libraries:</h2>
        <ul>
            {{#each libraries}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <hr>
        <h2>Tools:</h2>
        <ul>
            {{#each tools}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <hr>
        <h2>Community:</h2>
        <ul>
            {{#each community}}
            <li>{{name}} ({{type}})</li>
            {{/each}}
        </ul>
    </section>

</div>
